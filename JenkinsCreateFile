pipeline {
  agent any
  
  environment {
    DEMO='1.6'
  }
  
  stages {
    stage('Stage Test') {
      steps {
        echo "This is build number $BUILD_NUMBER of demo $DEMO"
      }
    }
    stage('Create File') {
      steps {
        echo "Creating new file"
        writeFile file: 'teste-result.txt', text: 'test passed' 
      }
    }
   }
}
